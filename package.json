{
  "name": "fuse-native",
  "version": "2.2.6",
  "description": "Fully maintained fuse bindings for Node that aims to cover the entire FUSE api",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist/",
    "index.d.ts",
    "build/",
    "bin.js",
    "binding.gyp",
    "README.md"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "fuse-native": "./bin.js"
  },
  "scripts": {
    "install": "node-gyp-build",
    "build": "tsc -p .",
    "prepare": "npx tsc -p .",
    "test": "tape test/*.js",
    "prebuild": "prebuildify --napi --strip",
    "prebuild-ia32": "prebuildify --napi --strip --arch=ia32",
    "configure": "NODE=$(which node) && sudo -E $NODE ./bin.js configure || true"
  },
  "gypfile": true,
  "dependencies": {
    "fuse-shared-library": "^1.0.2",
    "nanoresource": "^1.3.0",
    "napi-macros": "^2.0.0",
    "node-gyp-build": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "concat-stream": "^2.0.0",
    "prebuildify": "^3.0.4",
    "standard": "^13.1.0",
    "tape": "^4.12.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fuse-friends/fuse-native.git"
  },
  "author": "Mathias Buus (@mafintosh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fuse-friends/fuse-native/issues"
  },
  "homepage": "https://github.com/fuse-friends/fuse-native"
}
